{
  "manifest_version": 2,
  "name": "WaveTab",
  "version": "1.0",
  "description": "Color Wave accompany you Every New Tab!",
  "icons": { "128": "./img/icon_128.png" },
  "background": {
    "persistent": false,
    "scripts": ["./js/background.js"]
  },
  "permissions": [
    "activeTab"
  ],
  "chrome_url_overrides": {
    "newtab": "tab.html"
  },
  "browser_action": {
    "default_title": "",
    "default_icon": "./img/icon_128.png",
    "default_popup": "popup.html"
  }
}